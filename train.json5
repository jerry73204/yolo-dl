{
    "dataset": {
        "dataset_dir": "/mnt/client/aeon/coco-2017",
        "dataset_name": "train2017",
        "whitelist_classes": ["person", "bicycle", "car", "motorcycle", "bus", "truck"],
        "image_size": 256,
    },
    "logging": {
        "dir": "/mnt/client/aeon/yolo-dl-logs",
        "save_images": false,
    },
    "preprocessor": {
        "cache_dir": "/home/centos/tmp/yolo-dl-cache",
        "mosaic_prob": 0.3,
        "mosaic_margin": 0.4,
        "affine_prob": 0.3,
        "rotate_degrees": 10,
        "translation": 0.1,
        "scale": [0.9, 1.1],
        "shear": 1.1,
        "horizontal_flip": true,
        "vertical_flip": true,
    },
    "training": {
        "batch_size": 84,
        "default_minibatch_size": 84,
        "match_grid_method": "Rect4",
        "iou_kind": "GIoU",
        "initial_step": 0,
        "momentum": 0.9,
        "weight_decay": 0.0005,
        "lr_schedule": {
            "type": "StepWise",
            "steps": [
                [0, 0.01],
                [400000, 0.001],
                [450000, 0.0001],
            ],
        },
        "save_checkpoint_steps": 1,
        "load_checkpoint": {
            "type": "Disabled",
        },
        "enable_multi_gpu": false,
        "master_device": "cuda(0)",
        "workers": [
            {
                "device": "cuda(0)"
            }
        ]
    },
}
